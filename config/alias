# emacs
alias e="emacs -nw"

# git
alias gs="git status"
alias gsw="git switch"
alias gd="git diff"
alias gl="git log --pretty=oneline"
alias ga="git add"
alias gc="git commit"
alias gcp="git cherry-pick"
alias gp="git push"
alias gco="git checkout"
alias gpl="git pull"
alias grb="git rebase"
alias grs="git restore"
